{
  "name": "@brainhubeu/license-auditor-cli",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && npm run rename:esm",
    "rename:esm": "node ../../scripts/fix-mjs.js",
    "clean": "rimraf dist && rimraf *.tsbuildinfo",
    "dev": "tsc --watch",
    "cli": "tsx src/cli.tsx",
    "cli:init": "tsx src/cli.tsx init",
    "render-mock": "tsx --watch src/mock-render.tsx"
  },
  "files": ["dist"],
  "dependencies": {
    "@brainhubeu/license-auditor-core": "*",
    "@license-auditor/data": "*",
    "@license-auditor/package-manager-finder": "*",
    "cosmiconfig": "9.0.0",
    "ink": "5.0.1",
    "ink-select-input": "6.0.0",
    "ink-spinner": "5.0.0",
    "pastel": "3.0.0",
    "react": "18.3.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@license-auditor/typescript-config": "*",
    "@types/react": "18.3.11",
    "chalk": "5.2.0",
    "ink-testing-library": "3.0.0",
    "tsx": "4.19.1",
    "typescript": "5.6.2"
  }
}

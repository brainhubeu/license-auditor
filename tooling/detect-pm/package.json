{
  "name": "detect-pm",
  "version": "1.0.0",
  "description": "Detect which package manager you're using (npm, yarn classic, yarn modern, pnpm, or bun)",
  "originalRepository": "https://github.com/egoist/detect-package-manager.git",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "keywords": [
    "npm",
    "yarn",
    "yarn-classic",
    "yarn-modern",
    "package-manager",
    "detect",
    "check",
    "pnpm"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "node -r sucrase/register test.ts",
    "prepublishOnly": "npm run build"
  },
  "author": "https://github.com/brainhubeu/",
  "contributors": ["egoist <0x142857@gmail.com>"],
  "license": "MIT",
  "dependencies": {
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.4",
    "ava": "^3.15.0",
    "sucrase": "^3.20.3",
    "tsup": "^5.4.4",
    "typescript": "^4.4.4",
    "uvu": "^0.5.2"
  },
  "engines": {
    "node": ">=12"
  }
}
